  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "format": "yarn lint:fix && yarn prettier:fix",
    "format:check": "yarn lint && yarn prettier",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typechain": "typechain --target ethers-v6 --out-dir src/typing/contracts src/assets/abis/*.json",
    "postinstall": "yarn typechain",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org eco-qp --project ecoism-server ./dist && sentry-cli sourcemaps upload --org eco-qp --project ecoism-server ./dist"
  },
